{
    "name": "frontastic/payment-adyen",
    "description": "Frontastic Adyen payment integration",
    "version": "1.0.0-beta.1",
    "license": "None",
    "type": "library",
    "repositories": [
        {
            "type": "path",
            "url": "../../*"
        },
        {
            "type": "path",
            "url": "../../../*"
        }
    ],
    "config": {
        "platform": {
            "php": "7.4"
        },
        "discard-changes": true
    },
    "require":      {
        "php": ">=7.4",
        "ext-json": "*",

        "symfony/symfony": "^4.1",

        "frontastic/catwalk": "^1.0.0@dev",
        "frontastic/common": "^1.0.0@alpha",
        "adyen/php-api-library": "^7.0"
    },
    "require-dev":  {
        "frontastic/apidocs": "^1.0.0@alpha",
        "phpunit/phpunit": "@stable",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "@stable",
        "phploc/phploc": "@stable",
        "squizlabs/php_codesniffer": "@stable",
        "sebastian/phpcpd": "@stable",
        "phake/phake": "^3.0",
        "phpstan/phpstan": "^0.12.18",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-symfony": "^0.12.6",
        "phpstan/phpstan-beberlei-assert": "^0.12.2"
    },
    "autoload": {
        "psr-4": {
            "Frontastic\\Payment\\": "src/php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Frontastic\\Payment\\": "test/php/"
        }
    }
}
